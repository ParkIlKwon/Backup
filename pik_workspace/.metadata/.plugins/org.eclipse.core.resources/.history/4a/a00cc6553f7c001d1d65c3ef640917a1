import java.util.Scanner;

public class 연습용종합장_보조 {

	public static void main(String[] args) {

		int p1 [] = {1,1};
		int p2 [] = {2,2};
		
		int temp = p2[1];
		
		temp = 3; 
		p2[1] = temp;
		
		
		System.out.println(p2[1]);
		
		
		temp = p1[1];
		temp = 0;
		
		
		
		
		System.out.println(p2[1]);
	
	}
}