package 클래스배열;

public class 클배EX {

	public static void main(String[] args) {
		
		Student s = new Student();
		
		Object str [] = {s.number};
		
		str[0] = 3;
		
		System.out.println(s.number);
		
		int b = 0 ;
		Object obj[] = {b};
		
		obj[0] = 1;
		
		System.out.println(b);
		
		
		
		
		
		
		
		
		
		

	}

}
