package Longvinter;

import java.util.ArrayList;

public class DAO {

	final int size = 10;
	final int home = 10;
	VO [][] map = new VO[size][size];
	ArrayList<Integer>indexNumber = new ArrayList<Integer>();
	
	DAO(){
		
		dao = new DAO();
		for (int i = 0; i < map.length; i++) {
			for (int k = 0; k < map[i].length; k++) {
				map[i][k] = new VO();
			}
		}
		
		map[0][0].object=home;
		
	}
	
}
